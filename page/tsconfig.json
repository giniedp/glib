{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../dist/examples",
    "incremental": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "noFallthroughCasesInSwitch": true,
    "lib": ["es6", "dom", "es2015.promise", "es2015.collection", "es2015.iterable"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "target": "es6",
    "paths": {
      "@gglib/*": ["./../dist/packages/*"]
    }
  }
}
