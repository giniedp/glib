{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist/framework",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@glib/*": ["./*"]
    },
    "rootDir": ".",
    "sourceMap": true,
    "inlineSourceMap": false,
    "lib": ["es6", "dom", "es2015.promise", "es2015.collection", "es2015.iterable"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "target": "es6",
    "typeRoots": [
      "../npde_modules/@types"
    ],
    "types": [
      "jasmine",
      "webgl2"
    ]
  },
  "exclude": [

  ]
}
